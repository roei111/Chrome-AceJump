{
  "name": "acejump",
  "version": "1.2.2",
  "description": "AceJump allows you to quickly navigate any visible links",
  "private": true,
  "scripts": {
    "build": "webpack",
    "bundle": "webpack && web-ext build --source-dir ./dist",
    "watch": "webpack --watch",
    "start": "web-ext run --source-dir ./dist"
  },
